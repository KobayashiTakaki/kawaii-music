<%= link_to "admin index", admin_root_path %>
<h1>Tracks</h1>
<%= form_tag import_admin_tracks_path, multipart: true do %>
  <%= file_field_tag :file %>
  <%= submit_tag "import" %>
<% end %>
<div class="tracks-list">
  <%= render partial: 'admin/tracks/track_card', collection: @tracks, as: "track" %>
</div>
